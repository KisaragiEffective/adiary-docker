{
  "extends": [
    "config:base"
  ],
  "regexManagers": [
    {
      "fileMatch": ["adiary/Dockerfile"],
      "matchStrings": ["datasource=(?<datasource>.*?) depName=(?<depName>.*?) versioning=(?<versioning>.*?)?\nARG .+_VERSION=\"(?<currentValue>.*?)\"\n"],
      "versioningTemplate": "git",
      "datasourceTemplate": "github-releases"
    }
  ]
}
