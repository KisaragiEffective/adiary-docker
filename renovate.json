{
  "extends": [
    "config:base"
  ],
  "regexManagers": [
    {
      "fileMatch": ["adiary/Dockerfile"],
      "matchStrings": ["datasource=(?<datasource>.*?) depName=(?<depName>.*?)( versioning=(?<versioning>.*?))?\nARG .*?_VERSION=\"(?<currentValue>.*?)\""],
      "versioningTemplate": "regex:^v(?<major>\d+)\.(?<minor>\d+)(?<prerelease>[^.-]+)?$",
      "datasourceTemplate": "{{#if datasource}}{{{datasource}}}{{else}}github-releases{{/if}}"
    }
  ]
}
