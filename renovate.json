{
  "extends": [
    "config:base"
  ],
  "regexManagers": [
    {
      "fileMatch": ["adiary/Dockerfile"],
      "matchStrings": ["depName=(?<depName>.*?)?\\s.*?_COMMIT=\"(?<currentValue>)(?<currentDigest>.*?)\""],
      "versioningTemplate": "git",
      "datasourceTemplate": "git-refs"
    }
  ]
}
